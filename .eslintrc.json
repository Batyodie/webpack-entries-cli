{
  "plugins": [],
  "extends": [
    "eslint-config-airbnb-base"
  ],
  "rules": {
    "no-var": "error",
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-console": 1,
    "no-unused-vars": 1,
    "func-names": "off",
    "no-restricted-syntax": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "alwais"
      }
    ]
  }
}
